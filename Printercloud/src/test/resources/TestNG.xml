<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="UHC Automation Script" parallel="false">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="76.0"/>
    <parameter name="url" value="https://http.printercloud.com/admin/index.php"/>




    <test name = "Test" enabled ="true">
        <classes>
            <class name="testpage.PositiveTestLogIn"/>
            <class name="testpage.NegativeTestLogIn"/>
            <class name="testpage.NegativeTestLogIn2"/>
            <class name="testpage.NegativeTestLogIn3"/>
        </classes>
    </test>

</suite>